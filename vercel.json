{
    "version": 2,
      "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.4" }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/assets/"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        },
        {
            "src": "/img/(.*)",
            "dest": "/public/img/"
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/js/"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https:xxxxxxx.vercel.app", 

        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",

        "MAIL_MAILER": "smtp",
        "MAIL_HOST": "smtp.postmarkapp.com",
        "MAIL_PORT": "2525",
        "MAIL_USERNAME": "", 
        "MAIL_PASSWORD": "", 
        "MAIL_ENCRYPTION": "tls",
        "MAIL_FROM_ADDRESS": "", 
        "MAIL_FROM_NAME": "" 
    }
}